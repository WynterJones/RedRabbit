generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model communities {
  id   Int     @default('reddit_communities_id_seq') @id
  name String? @unique
}

model posts {
  attached_image  String?
  community_id    Int?
  created_at      String?
  id              Int       @default('posts_id_seq') @id
  included_image  String?
  included_link   String?
  sentiment_score Float?
  snippet         String?
  title           String?
  url             String?   @unique
}
